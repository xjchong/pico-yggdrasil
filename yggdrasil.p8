--drawing

function draw_game()
 palt(0, false)
 cls(0)
 map()

 draw_mobs()
 draw_toasts()
 
 pal(0,131,1) --background color
 pal(5,3,1) --disabled color
 pal(6,6,1) --player color
 pal(4,134,1) --wall color
 pal(8,8,1) --mob color
 --pal(10, 9, 1) --interactibles highlight color
end

function draw_mobs()
 for m in all(mobs) do
  local clr = nil
  if (m.flash>0) clr=7
  m.flash-=1
  
  draw_sprite(
   get_anim_sprite(m.anim),
   m.x*8+m.xo,
   m.y*8+m.yo,
   clr, 
   m.flipped
  )
 end
end

function draw_dist_map(dmap)
 for pos=1,256 do
  local x,y=pos_to_xy(pos)
  local dist=dmap[pos]
  if dist then
   print(tostring(dist),x*8,y*8,7)
  end
 end
end

