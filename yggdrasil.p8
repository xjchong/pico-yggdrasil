__lua__
function _init()
 frame = 0
 btn_bfr = nil
 p_sprs, is_p_flip = {1, 2, 3, 4}, false
 
 --x and y deltas
 dxs, dys = {-1, 1, 0, 0, 1, 1, -1, -1}, {0, 0, -1, 1, -1, 1, 1, -1}
 
 --delegates for update functions
 _upd_fn, _draw_fn = update_game, draw_game
 
 start_game()
end


function _update60()
 _upd_fn()
end


function _draw()
 frame += 1
 _draw_fn()
 draw_windows()
end

function start_game()
 mobs={}
 player=add_mob(1,3,7)
 add_mob(2,4,5)
 for x=11,13 do
  for y=9,11 do
   add_mob(2,x,y)
  end
 end
 
 toasts = {}
 windows = {}
 msg_window = nil
end



-->8
--updates

function update_game()
 load_btn_bfr()
 
 if btn_bfr then
  handle_btn(btn_bfr)
  btn_bfr = nil
 end
 
 --handle any mob deaths.
 for m in all(mobs) do
  if m.hp<=0 then
   del(mobs, m)
  end
 end 
end

function update_anim()
 load_btn_bfr()
 
 if player.xo==0 and player.yo==0 then
  _upd_fn = update_game
  return
 end
 
 _upd_fn = update_anim
 player.xo = approach(player.xo,0,player.anim_spd)
 player.yo = approach(player.yo,0,player.anim_spd)
end

function update_gameover()

end
