function load_btn_bfr()
 for i=0, 5 do
  if btnp(i) then
   btn_bfr = i
   return
  end
 end
end

function handle_btn(_btn)
 if (not _btn) return
 
 if msg_window then
  if _btn>3 then
   msg_window.dur = 0
   msg_window = nil
  end
 elseif _btn<4 then
  local did_act = move_mob(player,dxs[_btn+1],dys[_btn+1])
  if did_act then
   p_dist_map=get_dist_map(
    mob_pos(player)
   )
   update_ai()
  end
 end
end

