--mobs

--[[
the index is the mob type.
e.g, first entry of each these
tables is for the player (type 1).

mobs:
 1-player
 2-slime
--]] 

mob_hps={5,2}
mob_atks={1,1}
mob_anims={{1,2,3,4},{5,6,5,7}}

function is_player(mob)
 return mob.typ==1
end

function add_mob(_typ,_x,_y)
 local m={
  typ=_typ,
  x=_x,
  y=_y,
  xo=0, --x offset, used for animation
  yo=0, --y offset, used for animation
  hp=mob_hps[_typ],
  atk=mob_atks[_typ],
  anim=mob_anims[_typ],
  anim_spd=0,
  flash=0,
  flipped=false,
 }
 
 return add(mobs,m)
end

function get_mob(x,y)
 for m in all(mobs) do
  if m.x==x and m.y ==y then
   return m
  end
 end
end

function hit_mob(atkr,defr)
 defr.flash=8
 defr.hp-=atkr.atk
 
 show_toast(
  "-"..tostring(atkr.atk),
  defr.x*8,
  defr.y*8,
  10 --toast color
 )
end
