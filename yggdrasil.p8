pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
 frame = 0
 p_sprs, is_p_flip = {1, 2}, false
 
 --x and y deltas
 dxs, dys = {-1, 1, 0, 0}, {0, 0, -1, 1}
 
 --delegates for update functions
 _upd_fn, _draw_fn = update_game, draw_game
 
 start_game()
end


function _update60()
 _upd_fn()
end


function _draw()
 frame += 1
 _draw_fn()
end

function start_game()
 --player x/y, x/y offsets, orientation
 p_x, p_y = 3, 6
 p_xo, p_yo = 0, 0 
 p_flip = false
end



-->8
--updates

function update_game()
 for i=1, 4 do
  if btnp(i-1) then
   move_p(dxs[i], dys[i])
  end
 end
end

function update_anim()
 if upd_anim_i == 5 then
  upd_anim_i = nil
  _upd_fn = update_game
  return
 elseif upd_anim_i == nil then
  upd_anim_i = 1
  _upd_fn = update_anim
 end

 p_xo = p_anim[upd_anim_i]
 p_yo = p_anim[upd_anim_i+4]
 upd_anim_i += 1
end

function update_gameover()

end
-->8
--draws

function draw_game()
 palt(0, false)
 cls(0)
 map()
 
 draw_sprite(
  get_anim_sprite(p_sprs), 
  p_x*8+p_xo, 
  p_y*8+p_yo, 
  7, --player color
  p_flip
 )
 
 pal(0, 131, 1) --background color
 pal(5, 3, 1) --disabled color
 pal(7, 135, 1) --player color
 pal(13, 134, 1) --wall color
end

function draw_gameover()

end
-->8
--draw tools

function get_anim_sprite(sprites)
 --divide frame by different values for different speeds
 return sprites[flr(frame/32)%#sprites+1]
end

function draw_sprite(_sprite, _x, _y, _color, _flip)
 pal(7, _color) --this only works if all sprites are the same color by default
 spr(_sprite, _x, _y, 1, 1, _flip)
 pal()
end

-- multiplies each element in array by x and then y factor for animation
function anim_array_factor(arr, x_factor, y_factor)
 local new_arr = {}
 for i=1, 4 do
  new_arr[i] = arr[i]*x_factor
  new_arr[i+4] = arr[i]*y_factor
 end
 
 return new_arr
end
-->8
--gameplay tools
-->8
--player movement

--[[
the way im doing animations
is with an array describing
how to interpolate the offset.

there will be 4 frames for an animation.
the actual animation array needs
to have 8 values, {x,x,x,x,y,y,y,y}.

upd_anim_i is the index that will track
which frame of the animation we are on.
--]]

walk_anim = {-6,-4,-2,0}
bump_anim = {1,2,1,0}
p_anim = nil
upd_anim_i = nil

function move_p(dx, dy)
 local dest_x, dest_y = p_x+dx, p_y+dy
 local tile = mget(dest_x, dest_y)

 --handle player orientation
 if (dx<0) p_flip = true
 if (dx>0) p_flip = false
 
 if fget(tile, 0) then
  --not walkable
  p_anim = anim_array_factor(bump_anim, dx, dy)
  update_anim() 
 else
  p_x += dx
	 p_y += dy
  p_anim = anim_array_factor(walk_anim, dx, dy)
	 update_anim()
 end
 
 if fget(tile, 1) then
  --interactible
  handle_interact(tile, dest_x, dest_y)
 end
end

function handle_interact(tile, x, y)
 if tile==202 then
  --vases
  mset(x, y, 192)
 elseif tile==203 then
  --doors
  mset(x, y, 219)
 elseif tile==204 or tile==205 then
  --chests
  mset(x, y, tile+16)
 elseif tile==206 then
  --upstairs
 elseif tile==222 then
  --signs
 end
end
