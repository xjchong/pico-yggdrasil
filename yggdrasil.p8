--misc

function oob(x,y)
 return x<0 or x>15 or y<0 or y>15
end

--[[
mode can be one of:
 "move": would this pos be movable to
]]--
function blocked(x,y,mode)
 local tile=mget(x,y)
 if mode=="move" then
  return oob(x,y) 
   or fget(tile,0)
   or get_mob(x,y)
 end
end

